<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Stage_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Stage_App"
    xmlns:v="clr-namespace:Stage_App.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Listar"
        ContentTemplate="{DataTemplate v:ListadoOrganizadores}"
        Route="ListadoOrganizadores" />
    <ShellContent
        Title="Nuevo organizador"
        ContentTemplate="{DataTemplate v:organizador}"
        Route="organizador" />



</Shell>
